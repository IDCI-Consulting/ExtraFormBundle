{
    "name": "idci/extra-form-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle that expand existing FormType feature to allow a dynamic form generation",
    "keywords": ["form","form type","dynamic","configuration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@idci-consulting.fr",
            "homepage": "http://www.idci-consulting.fr"
        }
    ],
    "require": {
        "friendsofsymfony/rest-bundle": "~1.5|^2.0",
        "google/recaptcha": "^1.1",
        "gregwar/captcha-bundle": "~2.0",
        "idci/asset-loader-bundle": "~2.1",
        "jms/serializer-bundle": ">=1.1",
        "php": "^5.6|~7.0",
        "symfony/dependency-injection": "^3.3|^4.0",
        "symfony/event-dispatcher": "^3.3|^4.0",
        "symfony/form": "^3.3|^4.0",
        "symfony/http-foundation": "^3.3|^4.0",
        "symfony/http-kernel": "^3.3|^4.0",
        "symfony/options-resolver": "^3.3|^4.0",
        "symfony/security": "^3.3|^4.0",
        "symfony/validator": "^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0"
    },
    "suggest": {
        "idci/step-bundle": "Allow to use the extra form mechanism in a step workflow"
    },
    "autoload": {
        "psr-0": { "IDCI\\Bundle\\ExtraFormBundle": "" }
    },
    "target-dir": "IDCI/Bundle/ExtraFormBundle"
}
